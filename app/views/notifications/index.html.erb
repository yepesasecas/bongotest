<h1>Notifications Received: <%= Notification.count %></h1>
<h1>Listing notifications</h1>

<table>
  <thead>
    <tr>
      <th>Created</th>
      <th>Status</th>
      <th>Order</th>
      <th>Partner_key</th>
      <th>Server IP</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= distance_of_time_in_words notification.created_at, Time.now%> ago</td>
        <td><%= notification.status %></td>
        <td><%= notification.order %></td>
        <td><%= notification.partner_key %></td>
        <td><%= notification.ip %></td>
      </tr>
    <% end %>
  </tbody>
</table>